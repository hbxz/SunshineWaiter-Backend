{
    "dishes": [
      {{#repeat 20}}
        {
          "_id": {{@index}},
          "menuItem": {
            "title": "{{food}}"
          },
          "amount": {{int 1 4}},
          "placedBy": {{int 1 10}},
          "status": "PLACED",
          "order" : {
            "_id": "o{{@index}}",
            "createAt": "{{time '00:00' '07:00' 'HH:mm:ss'}}"
          }
        }
      {{/repeat}}
    ],
    "requests": [
      {{#repeat 20}}
      {
        "_id": {{@index}},
        "receiveTime": "{{time '00:00' '07:00' 'HH:mm:ss'}}",
        "tableId": {{int 1 10}},
        "finishTime": null
      }
      {{/repeat}}
    ]
}